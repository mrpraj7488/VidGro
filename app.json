{
  "expo": {
    "name": "VidGro",
    "slug": "vidgro-watch",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "vidgro",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.vidgro.watch",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.vidgro.watch",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#0A0E1A"
      }
    },
    "web": {
      "bundler": "webpack",
      "output": "single"
    },
    "plugins": ["expo-router", "expo-font", "expo-web-browser"],
    "experiments": {
      "typedRoutes": true
    }
  }
}