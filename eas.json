{
  "cli": {
    "version": ">= 5.0.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "NODE_ENV": "development",
        "EXPO_PUBLIC_API_BASE_URL": "https://admin.vidgro.com/api"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "NODE_ENV": "production",
        "EXPO_PUBLIC_API_BASE_URL": "https://admin.vidgro.com/api"
      },
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "env": {
        "NODE_ENV": "production",
        "EXPO_PUBLIC_API_BASE_URL": "https://admin.vidgro.com/api"
      },
      "android": {
        "buildType": "app-bundle",
        "gradleCommand": ":app:bundleRelease"
      },
      "ios": {
        "bundleIdentifier": "com.vidgro.watch"
      }
    },
    "production-obfuscated": {
      "extends": "production",
      "env": {
        "NODE_ENV": "production",
        "ENABLE_OBFUSCATION": "true",
        "EXPO_PUBLIC_API_BASE_URL": "https://admin.vidgro.com/api"
      },
      "android": {
        "buildType": "app-bundle",
        "gradleCommand": ":app:bundleRelease"
      },
      "ios": {
        "bundleIdentifier": "com.vidgro.watch"
      }
    }
  },
  "submit": {
    "production": {}
  }
}